<Screen1>:
    name:'screen1'

    BoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title: 'CheckLists'
            left_action_items: [["menu", lambda x: nav_drawer.toggle_nav_drawer()]]
            right_action_items: [["plus", lambda x: app.start_checklist()]]
            size_hint_y: 0.11
            elevation:5

        Widget:

    ScrollView:
        size_hint: (1.10, 0.90)
        pos_hint: {'center_x': .55}

        
        MDList:
            id: checklists
        

    MDNavigationDrawer:
        id: nav_drawer
        type: "standard"

        ContentNavigationDrawer:
            orientation: 'vertical'
            padding: "8dp"
            spacing: "8dp"

            Image:
                id: avatar
                size_hint: (1,1)
                source: "mine.jpg"

            MDLabel:
                text: app.nome_perfil
                font_style: "Subtitle1"
                size_hint_y: None
                height: self.texture_size[1]

            MDLabel:
                text: app.email_perfil
                size_hint_y: None
                font_style: "Caption"
                height: self.texture_size[1]


            DrawerList:
                id: md_list
                
                MDList:
                    OneLineIconListItem:
                        text: "CheckLists"
                        on_release: root.manager.current = 'screen1'
                    
                        IconLeftWidget:
                            icon: "inbox"
                    
                            
                    OneLineIconListItem:
                        text: "Minhas CheckList"
                        on_release: root.manager.current = 'screen2'
                    
                        IconLeftWidget:
                            icon: "inbox"

                        
                    OneLineIconListItem:
                        text: "Nova CheckList"
                        on_release: root.manager.current = 'screen5'
                        
                    
                        IconLeftWidget:
                            icon: "inbox-arrow-up"
                            
                    
                    OneLineIconListItem:
                        text: "Meu Perfil"
                    
                        IconLeftWidget:
                            icon: "account"

                    OneLineIconListItem:
                        text: "Sair"
                    
                        IconLeftWidget:
                            icon: "exit-to-app"
    
    
