<WelcomeScreen>:
    name : 'welcomescreen'
    
    BoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title: 'Paware Softwares'
            right_action_items: [["exit-run", lambda x: app.show_alert_dialog()]]
            elevation:4

        Widget:

    MDFloatingActionButton:
        icon:'arrow-right'
        md_bg_color:app.theme_cls.primary_color
        user_font_size : '15sp'
        pos_hint: {'center_x':0.5,'center_y':0.47}
        on_press:
            root.manager.current = 'usernamescreen'
            root.manager.transition.direction = 'left'
    MDProgressBar:
        value:30
        pos_hint:{'center_y' : 0.02}

<UsernameScreen>:
